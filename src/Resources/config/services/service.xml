<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="PostLabelCenter\Services\GreenFieldService">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="PostLabelCenter\Services\OrderShippingHelper"/>
            <argument type="service" id="log_entry.repository"/>
            <argument type="service" id="Shopware\Core\System\StateMachine\StateMachineRegistry"/>
            <argument>%kernel.shopware_version%</argument>
        </service>

        <service id="PostLabelCenter\Services\OrderShippingHelper">
            <argument type="service" id="order.repository"/>
            <argument type="service" id="currency.repository"/>
            <argument type="service" id="salutation.repository"/>
            <argument type="service" id="country.repository"/>
            <argument type="service" id="order_delivery.repository"/>
            <argument type="service" id="plc_address_data.repository"/>
            <argument type="service" id="plc_order_labels.repository"/>
            <argument type="service" id="plc_shipping_services.repository"/>
            <argument type="service" id="plc_order_return_data.repository"/>
            <argument>%kernel.shopware_version%</argument>
        </service>
    </services>
</container>
